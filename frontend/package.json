{
  "name": "forest-client-frontend",
  "version": "0.0.1",
  "description": "Frontend for forest client project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/nr-forest-client.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "typescript",
    "js",
    "devops"
  ],
  "author": "Maria Martinez, Paulo Cruz, and Bruno Marchi",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bcgov/nr-forest-client/issues"
  },
  "homepage": "https://github.com/bcgov/nr-forest-client",
  "private": true,
  "scripts": {
    "start": "vite --port 8080",
    "build": "vue-tsc --noEmit && vite build",
    "prepreview": "npm run build",
    "preview": "vite preview --port 5050",
    "test": "vitest --coverage",
    "coverage": "npm run test:unit",
    "postcoverage": "npm run test:component",
    "pretest:component": "rm -rf .nyc_output .nyc_report && mkdir -p .nyc_output/processinfo",
    "test:unit": "vitest run --coverage",
    "test:component": "cypress run --component --headless",
    "posttest:component": "istanbul-combine -x \".nyc_report/**\" -d coverage -p both -r lcov -r html .vite_report/coverage-final.json .nyc_report/coverage-final.json",
    "test:e2e": "cypress run --headless",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "stub": "wiremock --enable-stub-cors --port 3000 --preserve-host-header --root-dir ./stub --verbose --global-response-templating"
  },
  "dependencies": {
    "@bcgov-nr/nr-fsa-theme": "^1.0.7",
    "@bcgov/bc-sans": "^1.0.1",
    "@carbon/icons": "^11.20.0",
    "@carbon/icons-vue": "^10.49.1",
    "@carbon/pictograms": "^12.18.0",
    "@carbon/styles": "^1.30.0",
    "@carbon/web-components": "^1.28.0",
    "@dsb-norge/vue-keycloak-js": "^2.2.0",
    "@vueuse/core": "^10.2.0",
    "axios": "^1.3.3",
    "core-js": "^3.29.0",
    "dotenv": "^16.0.0",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^21.1.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-istanbul": "^0.32.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.17.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.0.0",
    "istanbul-combine": "^0.3.0",
    "istanbul-lib-coverage": "^3.2.0",
    "istanbul-lib-instrument": "^5.2.1",
    "jsdom": "^22.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.0",
    "vite": "^4.0.0",
    "vite-plugin-istanbul": "^4.1.0",
    "vitest": "^0.32.2",
    "volar-service-vetur": "latest",
    "vue-eslint-parser": "^9.3.1",
    "vue-svg-loader": "^0.16.0",
    "vue-tsc": "^1.8.1",
    "wiremock": "^2.35.0"
  }
}
