{
  "name": "forest-client-frontend",
  "version": "0.0.1",
  "description": "Frontend for forest client project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/nr-forest-client.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "typescript",
    "js",
    "devops"
  ],
  "author": "Maria Martinez, Paulo Cruz, and Bruno Marchi",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bcgov/nr-forest-client/issues"
  },
  "homepage": "https://github.com/bcgov/nr-forest-client",
  "private": true,
  "scripts": {
    "start": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest --environment jsdom --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@dsb-norge/vue-keycloak-js": "^2.2.0",
    "@iconify-json/bi": "^1.1.15",
    "@popperjs/core": "^2.11.4",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^1.3.3",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.23.1",
    "bootstrap-vue-3": "^0.5.1",
    "core-js": "^3.28.0",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "serve": "^14.2.0",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^2.8.6",
    "vue": "^3.2.47",
    "vue-axios": "^3.4.1",
    "vue-tsc": "^1.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^21.1.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.3.0",
    "c8": "^7.13.0",
    "cypress": "^12.6.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "start-server-and-test": "^1.15.4",
    "typescript": "~4.9.5",
    "vitest": "^0.28.5"
  }
}
