{
  "name": "forest-client-frontend",
  "version": "0.0.1",
  "description": "Frontend for old growth project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/nr-old-growth-integration.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "typescript",
    "js",
    "devops"
  ],
  "author": "Catherine Meng, Maria Martinez",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bcgov/nr-old-growth-integration/issues"
  },
  "homepage": "https://github.com/bcgov/nr-old-growth-integration",
  "private": true,
  "scripts": {
    "start": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest --environment jsdom --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@dsb-norge/vue-keycloak-js": "^2.1.5-beta",
    "@iconify-json/bi": "^1.1.12",
    "@popperjs/core": "^2.11.4",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitest/coverage-c8": "^0.25.6",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^1.2.1",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.23.1",
    "bootstrap-vue-3": "^0.4.13",
    "core-js": "^3.8.3",
    "dotenv": "^16.0.0",
    "serve": "^14.1.2",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^2.8.6",
    "vue": "^3.2.31",
    "vue-axios": "^3.4.1",
    "vue-tsc": "^1.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.2.6",
    "c8": "^7.11.0",
    "cypress": "^12.0.2",
    "eslint": "^8.29.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.9.3",
    "vitest": "^0.25.6"
  }
}
