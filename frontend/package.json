{
  "name": "forest-client-frontend",
  "version": "0.0.1",
  "description": "Frontend for forest client project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/nr-forest-client.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "typescript",
    "js",
    "devops"
  ],
  "author": "Maria Martinez, Paulo Cruz, and Bruno Marchi",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bcgov/nr-forest-client/issues"
  },
  "homepage": "https://github.com/bcgov/nr-forest-client",
  "private": true,
  "scripts": {
    "start": "vite --host --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "start-server-and-test stub http://127.0.0.1:8080/ 'vite --host --port 3000'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "stub": "wiremock --enable-stub-cors --port 8080 --https-port 8081 --preserve-host-header --root-dir ./stub --verbose --global-response-templating",
    "precoverage": "npm run test:report:clean",
    "coverage": "npm run test:unit && npm run test:component && npm run test:e2e",
    "postcoverage": "npm run test:report:merge",
    "test:component": "cypress run --component --headless",
    "posttest:component": "mv reports/.nyc_report reports/component",
    "test:unit": "vitest run --coverage",
    "posttest:unit": "mv reports/.vite_report reports/unit",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress run --headless'",
    "posttest:e2e": "mv reports/.nyc_report reports/e2e",
    "test:report:merge": "lcov-result-merger 'reports/*/lcov.info' 'coverage/lcov.info'",
    "test:report:clean": "mkdir -p reports/.nyc_output/processinfo && mkdir -p coverage",
    "test:build": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress open'"
  },
  "dependencies": {
    "@bcgov-nr/nr-fsa-theme": "^1.1.2",
    "@bcgov/bc-sans": "^2.0.0",
    "@carbon/icons": "^11.20.0",
    "@carbon/icons-vue": "^10.49.1",
    "@carbon/pictograms": "^12.18.0",
    "@carbon/styles": "^1.30.0",
    "@carbon/web-components": "^1.28.0",
    "@vueuse/core": "^10.2.0",
    "axios": "^1.3.3",
    "dotenv": "^16.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.8",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/cypress": "^1.1.3",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-istanbul": "^0.33.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.17.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.0.0",
    "istanbul-lib-coverage": "^3.2.0",
    "istanbul-lib-instrument": "^5.2.1",
    "jsdom": "^22.0.0",
    "lcov-result-merger": "^4.1.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.0",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.0.0",
    "vite-plugin-istanbul": "^4.1.0",
    "vitest": "^0.33.0",
    "volar-service-vetur": "latest",
    "vue-eslint-parser": "^9.3.1",
    "vue-svg-loader": "^0.16.0",
    "vue-tsc": "^1.8.1",
    "wiremock": "^2.35.0"
  }
}
