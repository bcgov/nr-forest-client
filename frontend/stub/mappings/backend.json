{
  "mappings": [
    {
      "request": {
        "urlPattern": "/api/clients/name/(.*)",
        "method": "GET"
      },
      "response": {
        "status": 200,
        "bodyFileName": "response-autocomplete.json",
        "transformers": ["response-template"]
      }
    },
    {
      "request": {
        "urlPattern": "/api/clients/(XX|YY)(.*)",
        "method": "GET"
      },
      "response": {
        "status": 200,
        "bodyFileName": "response-details-{{regexExtract request.path.[2] '(XX|YY)(.*)' 'parts'}}{{parts.0}}.json",
        "transformers": ["response-template"]
      }
    },
    {
      "request": {
        "urlPattern": "/api/clients/ZZ(.*)",
        "method": "GET"
      },
      "response": {
        "status": 409,
        "body": "The Dupped Company already exists with the Incorporation number {{request.path.[2]}} and client number 999999",
        "transformers": ["response-template"]
      }
    },

    {
      "request": {
        "url": "/api/clients/activeCountryCodes?page=0&size=250",
        "method": "GET"
      },
      "response": {
        "status": 200,
        "bodyFileName": "response-countries.json"
      }
    },
    {
      "request": {
        "url": "/api/clients/activeContactTypeCodes?page=0&size=250",
        "method": "GET"
      },
      "response": {
        "status": 200,
        "body": "[{\"code\":\"P\",\"name\":\"Person\"}]"
      }
    },
    {
      "request": {
        "urlPattern": "/api/clients/activeCountryCodes/(.*)?page=0&size=250",
        "method": "GET"
      },
      "response": {
        "status": 200,
        "transformers": ["response-template"],
        "bodyFileName": "response-provinces-{{request.path.[3]}}.json"
      }
    }
  ]
}
