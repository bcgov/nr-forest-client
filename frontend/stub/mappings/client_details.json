      "name": "Client Details General Partnership - Default",
      "request": {
        "urlPattern": "/api/clients/details/g?(.*)",
        "method": "GET"
      },
      "response": {
        "status": 200,
        "bodyFileName": "response-clients-details-G.json",
        "transformers": [
          "response-template"
        ],
        "fixedDelayMilliseconds": 500
