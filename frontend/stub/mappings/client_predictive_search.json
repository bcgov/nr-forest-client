{
  "mappings": [    
    {
      "request": {
        "urlPattern": "/api/clients/predictive-search",
        "method": "GET",
        "queryParameters": {
          "keyword": {
            "matches": "^(.*)$"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "x-total-count": "57",
          "content-type": "application/json;charset=UTF-8",
          "Access-Control-Expose-Headers": "x-total-count"
        },
        "bodyFileName": "response-predictive-search.json",
        "transformers": [
          "response-template"
        ],
        "fixedDelayMilliseconds": 2500
      }
    }
  ]
}
