{
  "mappings": [    
    {
      "name": "Autocompete Predictive Search",
      "request": {
        "urlPath": "/api/clients/predictive-search",
        "method": "GET",
        "queryParameters": {
          "keyword": {
            "matches": "^(.*)$"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "content-type": "application/json;charset=UTF-8"
        },
        "bodyFileName": "response-client-predictive-search.json",
        "transformers": [
          "response-template"
        ],
        "fixedDelayMilliseconds": 1000
      }
    },
    {
      "name": "Autocompete Predictive Search CAR",
      "request": {
        "urlPath": "/api/clients/predictive-search",
        "method": "GET",
        "queryParameters": {
          "keyword": {
            "matches": "(?i)car"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "content-type": "application/json;charset=UTF-8"
        },
        "bodyFileName": "response-client-predictive-search-keyword-CAR.json"
      }
    }
  ]
}
